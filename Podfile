# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

target 'swiftUIApi' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for swiftUIApi
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxTest'
  pod 'RxBlocking'


  target 'swiftUIApiTests' do
    # Pods for testing
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'RxTest'
    pod 'RxBlocking'
  end

  target 'swiftUIApiUnitTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
